{
	"manifest_version": 2,
	"name": "My extension", 
	"version": "0.1",		
	"description": "Расширение, сохраняющее ссылки",

	"permissions": [
		"tabs",
		"storage"
	],

	"browser_action": {
		"default_title": "LinkSaver",
		"default_popup": "popup.html"		
	},

	"options_page": "options.html",

	"icons": {
		"16": "icon/icon16.png",
		"48": "icon/icon48.png",
		"128": "icon/icon128.png"
	}
}